namespace Pokedex.Models.Players
{
    internal class AIPlayer : Player
    {
        #nullable enable
        private static AIPlayer? _instance = null;
        #nullable restore
        public static AIPlayer Instance
        {
            get
            {
                if (_instance == null)
                {
                    _instance = new AIPlayer();
                }
                return _instance;
            }
        }
        public AIPlayer()
        : base("", Enums.Gender.O)
        {}
    }
}
